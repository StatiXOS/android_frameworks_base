<resources>

  <!-- Power menu items -->
  <java-symbol name="reboot_to_bootloader_title" type="string" />
  <java-symbol name="reboot_to_bootloader_message" type="string" />
  <java-symbol name="reboot_to_recovery_title" type="string" />
  <java-symbol name="reboot_to_recovery_message" type="string" />
  <java-symbol name="reboot_system_title" type="string" />
  <java-symbol name="reboot_system_message" type="string" />
  <java-symbol type="array" name="config_rebootActionsList" />
  <java-symbol type="array" name="values_globalActionsList" />
  <java-symbol type="color" name="power_menu_power_off" />
  <java-symbol type="color" name="power_menu_restart" />
  <java-symbol type="color" name="power_menu_bootloader" />
  <java-symbol type="color" name="power_menu_recovery" />
  <java-symbol type="color" name="power_menu_hot_reboot" />
  <java-symbol type="color" name="power_menu_screenshot" />
  <java-symbol type="color" name="power_menu_airplane_mode" />
  <java-symbol type="color" name="power_menu_airplane_mode_off" />
  <java-symbol type="color" name="power_menu_flashlight" />
  <java-symbol type="color" name="power_menu_settings" />
  <java-symbol type="color" name="power_menu_lockdown" />
  <java-symbol type="color" name="power_menu_screenrecord" />
  <java-symbol type="color" name="power_menu_user" />
  
  <!-- Whether the device supports Smart Pixels -->
  <java-symbol type="bool" name="config_enableSmartPixels" />

  <!-- Package Manager -->
  <java-symbol type="array" name="config_disabledComponents" />
  <java-symbol type="array" name="config_forceEnabledComponents" />

  <!-- App error dialog -->
  <java-symbol type="id" name="aerr_copy" />
  <java-symbol type="string" name="url_copy_success" />
  <java-symbol type="string" name="url_copy_failed" />

  <!-- Settings icon tint color -->
  <java-symbol type="color" name="settings_icon_tint_color" />

  <!-- Auto fill -->
  <java-symbol type="color" name="autofill_save_title_color" />
  <java-symbol type="color" name="autofill_autofill_save_custom_subtitle_color" />

  <!-- Notification info -->
  <java-symbol type="color" name="notification_info_button_text" />

  <!-- Toast text and bg colors -->
  <java-symbol type="color" name="toast_bg_color" />
  <java-symbol type="color" name="toast_text_color" />

  <!-- Forcefully tint notification icon text -->
  <java-symbol type="bool" name="config_useDarkBgNotificationIconTextTinting" />
  <java-symbol type="bool" name="config_useDarkBgNotificationTinting_override" />
  <java-symbol type="bool" name="config_notificationTinting_override" />

  <!-- Whether device supports an alt. ambient display -->
  <java-symbol type="bool" name="config_alt_ambient_display"/>

  <!-- SystemUI Restart -->
  <java-symbol type="string" name="systemui_restart_title" />
  <java-symbol type="string" name="systemui_restart_message" />

  <!-- Oreo's recents component activity -->
  <java-symbol type="string" name="config_recentsComponentNameOreo" />

  <!-- ADB notification -->
  <java-symbol type="string" name="adb_net_active_notification_title" />
  <java-symbol type="string" name="adb_both_active_notification_title" />
  <java-symbol type="string" name="adb_active_generic_notification_message" />

  <!-- Fastscroller -->
  <java-symbol type="color" name="fastscroller_text_color" />

  <!-- Device keyhandlers -->
  <java-symbol type="array" name="config_deviceKeyHandlerLibs" />
  <java-symbol type="array" name="config_deviceKeyHandlerClasses" />

  <!-- Proximity check on screen on -->
  <java-symbol type="bool" name="config_proximityCheckOnWake" />

  <!-- Proximity check timeout -->
  <java-symbol type="integer" name="config_proximityCheckTimeout" />

  <!-- Proximity check on screen on default -->
  <java-symbol type="bool" name="config_proximityCheckOnWakeEnabledByDefault" />

  <!-- Gesture Sensor -->
  <java-symbol type="bool" name="config_enableGestureService" />

  <!-- Battery LED -->
  <java-symbol type="bool" name="config_intrusiveBatteryLed" />
  <java-symbol type="bool" name="config_multiColorBatteryLed" />
  <java-symbol type="bool" name="config_multiColorNotificationLed" />
  <java-symbol type="bool" name="config_ledCanPulse" />

  <java-symbol type="anim" name="inv_app_in" />
  <java-symbol type="anim" name="inv_app_out" />

  <!-- System fonts -->
  <java-symbol type="array" name="config_fontPackages" />

  <!-- Notification icon/text dynamic tint -->
  <java-symbol type="bool" name="config_allowNotificationIconTextTinting" />
  <java-symbol type="color" name="notification_text_default_color" />
  <java-symbol type="color" name="notification_icon_default_color" />

  <!-- Notification sender text color -->
  <java-symbol type="color" name="sender_text_color" />

  <!-- Notification app name tint -->
  <java-symbol type="bool" name="config_allowNotificationAppNameTextTinting" />

</resources>

